{
    "builds": [{
        "src": "student_management_project/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.12.2" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "student_management_project/wsgi.py"
        }
    ]
}
